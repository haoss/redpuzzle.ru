doctype

html(lang="ru")
  
  -var active = 'Главная' //активный пункт меню, список в header.jade
  -var pageTitle = 'Главная' + ' - redpuzzle.ru' //title страницы для генерации верстки  

  include ./_include/_head

  body
    
    // Preloader
    include ./_include/_preloader
    
    include ./_include/_header
    
    // Slide block
    .slide
      .container
          
        .slide__form
          form(action="#")
            fieldset
              legend зарегистрироваться
              .row.row--slide-form
                .col-sm-6
                  .form-group.form-group--small.has-feedback.block-focus
                    label.hidden Имя
                    input(type="text" name="name" placeholder="Имя").form-control.input-focus
                    i.ion-alert-circled
                .col-sm-6
                  .form-group.form-group--small.has-feedback.block-focus
                    label.hidden Фамилия
                    input(type="text" name="name" placeholder="Фамилия").form-control.input-focus
                    i.ion-alert-circled
              .form-group.form-group--small.has-feedback.block-focus.has-error
                label.hidden Email
                input(type="email" name="name" placeholder="Email").form-control.input-focus
                i.ion-alert-circled
              .form-group.form-group--small.has-feedback.block-focus
                label.hidden Телефон
                input(type="text" name="name" placeholder="Телефон").form-control.input-focus
                i.ion-alert-circled
              .form-group.form-group--small.has-feedback.block-focus
                label.hidden Пароль
                input(type="password" name="name" placeholder="Пароль").form-control.input-focus
                i.ion-alert-circled
              button(type="submit").btn.btn--3d.btn--block
                span Разместить резюме
              p Вы можете прийти к нам в офис в вашем городе и мы все сделем за Вас.
              .form-group.form-group--checkbox.form-group--small
                label.hidden Согласен на обработку моих личных данных
                input#input4(type="checkbox" name="confidental" checked)
                i.ion-android-checkbox-outline-blank
                i.ion-android-checkbox-outline
                label(for="input4").label Согласен на передачу персональных данных
        .slide__content
          .slide__content-text
            span.span1 
              i Сервис для трудовых мигрантов
            br
            span.span2 
              i Redpuzzle и Красный Крест помогут 
            br
            span.span3 
              i тебе найти работу!
          img(src="./img/slide-img-1.png", alt="")
    
