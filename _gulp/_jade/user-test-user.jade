doctype

html(lang="ru")
  
  -var active = 'Список тестов 1' //активный пункт меню, список в header.jade
  -var pageTitle = 'Список тестов 1' + ' - redpuzzle.ru' //title страницы для генерации верстки  

  include ./_include/_head

  body
    
    // Preloader
    include ./_include/_preloader
    
    include ./_include/_header
    
    // Content
    .content.content--all
      .container
        
        // Breadcrumbs
        ul.breadcrumbs
          li
            a(href="#!") RedPuzzle.ru
          li
            span Название страницы
        
        .content__head.content__head--mobile
          button#mobileColBtn(type="button").content__head-button
            i.ion-gear-a
          h1 Список тестов
        
        // Mobile column
        #mobileCol.mobile__col
          .mobile__col-background
          .mobile__col-wrapper
            #mobileColWrapperBody.mobile__col-wrapper__body
              button#mobileColButton(type="button").mobile__col-button
                span
                  i.ion-gear-a
              #mobileColWrapperBody-div
                
        .row
          .col-md-3.hidden-sm.hidden-xs
            #mobileColContent.mobile__col-content
              div
                include ./_include/_profile-left
                  
          .col-md-9
            
            // Tests
            .tests
              form(action="#!")
                .tests__title Название теста определенной длины и сложности
                
                .tests__row
                  .tests__row-title
                    span 1.
                    | &nbsp;Вопрос за словесными горами в стране гласных и согласных живут рыбные тексты?
                  p Далеко-далеко за словесными горами в стране гласных и согласных живут рыбные тексты. Вдали от всех живут они в буквенных домах на берегу Семантика большого языкового океана. Маленький ручеек Даль журчит по всей стране и обеспечивает ее всеми необходимыми правилами. Эта парадигматическая страна, в которой жаренные предложения залетают прямо в рот.
                  .tests__answer
                    div
                      .form-group.form-group--radio.form-group--radio-medium
                        input#radioTest11(type="radio" name="radioTest11")
                        i.ion-android-radio-button-off
                        i.ion-android-checkmark-circle
                        label(for="radioTest11").label Ответ номер один
                    div
                      .form-group.form-group--radio.form-group--radio-medium
                        input#radioTest12(type="radio" name="radioTest11" checked)
                        i.ion-android-radio-button-off
                        i.ion-android-checkmark-circle
                        label(for="radioTest12").label Ответ номер два
                    div
                      .form-group.form-group--radio.form-group--radio-medium
                        input#radioTest13(type="radio" name="radioTest11")
                        i.ion-android-radio-button-off
                        i.ion-android-checkmark-circle
                        label(for="radioTest13").label Ответ номер три
                    div
                      .form-group.form-group--radio.form-group--radio-medium
                        input#radioTest14(type="radio" name="radioTest11")
                        i.ion-android-radio-button-off
                        i.ion-android-checkmark-circle
                        label(for="radioTest14").label Ответ номер четыре
                        
                .tests__row
                  .tests__row-title
