                button(type="button").btn.btn--tests
                  span отправить
    
                .tests__result
                  .tests__result-wrapper
                    div
                      .tests__result-title поздравляем, Тест пройден
                      .tests__result-number Ваш результат - 
                        strong 4.8
                        | &nbsp;балла
                    div
                      .tests__result-test осталось попыток - 
                        strong 3
                      .form-group.form-group--checkbox.no--m-bottom
                        input#input333(type="checkbox" name="add-resume" checked)
