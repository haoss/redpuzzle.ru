doctype

html(lang="ru")
  
  -var active = '1 резюме' //активный пункт меню, список в header.jade
  -var pageTitle = '1 резюме' + ' - redpuzzle.ru' //title страницы для генерации верстки  

  include ./_include/_head

  body
    
    // Preloader
    include ./_include/_preloader
    
    include ./_include/_header
    
    // Content
    .content.content--all
      .container
        
        // Breadcrumbs
        ul.breadcrumbs
          li
            a(href="#!") RedPuzzle.ru
          li
            span Название страницы
        
        .row
          .col-md-9
            
            // One-resume
            .one-resume__head
              .one-resume__head-img
                img(src="./img/one-resume-img.png", alt="")
                .one-resume__head-rating
                  span
                    i.ion-android-star
                    | 9.9
                .one-resume__head-label.visible-xs рекомендуем
              .one-resume__head-info
                .one-resume__head-label.hidden-xs рекомендуем
                .one-resume__head-title
                  | Иванов иван иванович
                  
                  .one-resume__head-icon
                    .catalog-resume__icon(data-balloon="телефон подтвержден" data-balloon-pos="up").catalog-resume__icon--active
                      i.ion-android-call
                      i.ion-checkmark-circled
                    .catalog-resume__icon(data-balloon="резюме подтверждено" data-balloon-pos="up").catalog-resume__icon--active
                      i.ion-document-text
                      i.ion-checkmark-circled
                    .catalog-resume__icon
                      a(href="#!").edit-block 
                        i.ion-compose
                    
                .one-resume__head-price Инженер-строитель, 40 000-80 000 руб.
                .one-resume__head-time Резюме от 10 сентября 2017
                
              .clear
              
              .button__group.button__group--one-resume
                div
                  a(href="#!" target="_blank").one-resume__doc
                    svg(xmlns='http://www.w3.org/2000/svg', version='1.2', viewbox='0 0 14 20', width='14', height='20')
                      g(fill='#ff0f36')
                        path(d='M7.19 15.02c-.663 0-1.04.637-1.04 1.496 0 .858.4 1.472 1.04 1.472.66 0 1.016-.638 1.016-1.497.024-.784-.355-1.47-1.017-1.47zm0 0M2.34 15.02c-.212 0-.354.023-.424.048v2.895c.07.025.212.025.33.025.876 0 1.443-.49 1.443-1.546 0-.933-.52-1.423-1.35-1.423zm0 0')
